{
  "name": "workshop-api-testing",
  "version": "1.0.0",
  "description": "This is a Workshop about Api Testing in JavaScript",
  "main": "index.js",
  "scripts": {
    "test": "mocha -t 5000 --reporter-options reportDir=report,reportFilename=ApiTesting"
  },
  "keywords": [
    "api-testing",
    "dojo",
    "practice"
  ],
  "author": "tomasalvarez <tomas.alvarez@perficient.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.4.0",
    "mocha": "^10.7.3",
    "mochawesome": "^7.1.3"
  },
  "mocha": {
    "diff": true,
    "extension": [
      "js",
      "cjs",
      "mjs"
    ],
    "package": "./package.json",
    "tiemout": "5000",
    "reporter": "mochawesome",
    "slow": "75",
    "watch-files": ["lib/**/*.js", "test/**/*.js"],
    "watch-ignore": ["lib/vendor"]
  },
  "mocha": {
    "diff": true,
    "extension": ["js", "cjs", "mjs"],
    "package": "./package.json",
    "tiemout": 5000
  },
  "dependencies": {
    "axios": "^1.7.7"
  }
}
